<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="label_style">
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#444444</item></style>
    <style name="CircleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="MyPageSingleItem" parent="">
        <item name="android:background">@drawable/bg_rounded_white_box</item>
        <item name="android:elevation">3dp</item>
        <item name="android:padding">16dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>

    <style name="MyPageContainerStyle" parent="">
        <item name="android:background">@drawable/bg_rounded_white_box</item>
        <item name="android:elevation">3dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:foreground">?attr/selectableItemBackground</item>
    </style>

    <style name="NotificationSwitchStyle" parent="Widget.MaterialComponents.CompoundButton.Switch">
        <item name="android:minWidth">64dp</item>
        <item name="android:minHeight">36dp</item>
        <item name="android:trackTint">@color/switch_track_selector</item>
        <item name="android:thumbTint">@color/switch_thumb_selector</item>
    </style>

</resources>